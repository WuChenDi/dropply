{
  "name": "@cdlab996/dropply-utils",
  "type": "module",
  "version": "1.0.0",
  "homepage": "https://github.com/WuChenDi",
  "author": "wudi <wuchendi96@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WuChenDi/dropply.git",
    "directory": "packages/utils"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@cdlab996/dropply-tsconfig": "workspace:*",
    "@types/node": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev"
  }
}
